{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/vscode-extension",
				"${workspaceFolder}/demo",
				"--profile empty"
			],
			"outFiles": ["${workspaceFolder}/vscode-extension/dist/**/*.js"]
		}
	]
}
